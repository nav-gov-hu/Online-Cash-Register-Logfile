<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.nav.gov.hu/OPGN/2.0" targetNamespace="http://schemas.nav.gov.hu/OPGN/2.0" elementFormDefault="qualified">
	<xsd:element name="ROWS">
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element name="LON" type="LON_tip" minOccurs="1" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
              Naplóállomány nyitása esetén a bejegyzés azonosító: LON
            </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="EVT" type="EVT_tip" minOccurs="1" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
