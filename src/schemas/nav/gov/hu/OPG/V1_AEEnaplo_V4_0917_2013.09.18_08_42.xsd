  <xsd:complexType name="Kifizetes_adatok">
    <xsd:sequence>
      <xsd:element name="POA" type="num20t_tip" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>a kifizetett forint készpénz összeg, egész forintban számolva</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="POC" type="Valuta_adatok" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>kifizetett valutaösszeg adatai</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="POB" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>banki utalással teljesített kifizetés összege egész forintban</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="num20t_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="POE" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>a kifizetés egyéb fizetőeszközzel (utalvány, csekk, stb.) teljesített összege egész forintban</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="num20t_tip"/>
        </xsd:simpleType>
      </xsd:element>
