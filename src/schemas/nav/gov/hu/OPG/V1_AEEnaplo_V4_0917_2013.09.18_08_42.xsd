        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TSZ" type="TSZ_tip">
        <xsd:annotation>
          <xsd:documentation>A bizonylatot kiállító adóalany adó törzsszáma</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
