<?xml version="1.0" encoding="UTF-8" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.nav.gov.hu/OPGN/1.0"
            targetNamespace="http://schemas.nav.gov.hu/OPGN/1.0" elementFormDefault="qualified">
  <xsd:element name="ROWS">
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element name="LON" maxOccurs="unbounded" minOccurs="1" type="LON_tip"/>
        <xsd:element name="EVT" type="EVT_tip" maxOccurs="unbounded" minOccurs="1"/>
        <xsd:element name="INF" type="INF_tip" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="STA" type="STA_tip" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="OPR" type="OPR_tip" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="TUD" type="TUD_tip" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="NYN" type="P10N" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="ESN" type="P10E" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="NFN" type="P11" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="PJN" type="P12" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="PBN" type="P13" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="PKN" type="P14" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="UKN" type="P21" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="KBN" type="P15" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="EBN" type="P16" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="SZN" type="P17" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="VBN" type="P18" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="EDN" type="P19" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="PCN" type="P20" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="NYT" type="P10N" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="EST" type="P10E" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="NFT" type="P11" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="PJT" type="P12" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="PBT" type="P13" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="PKT" type="P14" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="UKT" type="P21" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="KBT" type="P15" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="EBT" type="P16" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="SZT" type="P17" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="VBT" type="P18" maxOccurs="unbounded" minOccurs="0"/>
        <xsd:element name="EDT" type="P19" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element name="PCT" type="P20" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
	  <xsd:attribute name="semaver" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="LON_tip">
    <xsd:annotation>
      <xsd:documentation>szöveges állomány nyitásakor</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RSR" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>Naplón belüli folyamatos rekordsorszám 1-től</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DTS" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>Időbélyeg</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="APN">
        <xsd:annotation>
          <xsd:documentation>Pénztárgép AP száma</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="APszam_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="TSZ">
        <xsd:annotation>
          <xsd:documentation>Üzemeltető törzsszáma</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="TSZ_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="THT" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Társfelhasználó törzsszáma kétvállalkozós pénztárgépeknél</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="TSZ_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HSH" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>Ellenőrző kód</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EVT_tip">
    <xsd:annotation>
      <xsd:documentation>pénztárgépben bekövetkezett események naplózására</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RSR" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>Naplón belüli folyamatos rekordsorszám 1-től</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DTS" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>Időbélyeg</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EVC">
        <xsd:annotation>
          <xsd:documentation>Esemény kódja</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="LCL|PIN|POF|BDN|RST|BLS|BLF|TRI|TRF|CHO|MGI|MGO"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="EVD" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Esemény leíró adata</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="400"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HSH" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>Ellenőrző kód</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="INF_tip">
    <xsd:annotation>
      <xsd:documentation>pénztárgép mûszaki jellemzõirõl</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RSR" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>Naplón belüli folyamatos rekordsorszám 1-től</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DTS" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>Időbélyeg</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DTY">
        <xsd:annotation>
          <xsd:documentation>AEE gyártói típusnév</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="szov200_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DRV">
        <xsd:annotation>
          <xsd:documentation>AEE hardver verzió</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="szov200_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DFW">
        <xsd:annotation>
          <xsd:documentation>AEE szoftver verzió</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:length value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DFH" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>AEE egységben futó szoftver SHA256 algoritmussal kiszámított ellenőrző kódja</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DFT" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>Az AEE egységben futó szoftver legutóbbi frissítésének időpontja.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DMT">
        <xsd:annotation>
          <xsd:documentation>Az AEE egységben lévő modem kiolvasott típus megnevezése</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DMF" type="szov200_tip">
        <xsd:annotation>
          <xsd:documentation>Az AEE egységben lévő modem kiolvasott szoftver-verziója.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DME" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>Az AEE egységben lévő modem kiolvasott IMEI száma.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DMS" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>Az AEE egységben lévő SIM kiolvasott IMSI száma.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TZC">
        <xsd:annotation>
          <xsd:documentation>Ptg. Telephely irányítószám</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="6"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="TCY" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>Ptg. Telephely település</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TKN" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>Ptg. Telephely közterület neve</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TKJ" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>Ptg. Telephely közterület jellege</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="THH">
        <xsd:annotation>
          <xsd:documentation>Ptg. Telephely házszám</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="20"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DPH" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>A pénztárgép hardver gyártói típusmegjelölése</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DPV" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>A pénztárgép hardververziója</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DPF" type="szov50_tip">
        <xsd:annotation>
          <xsd:documentation>A pénztárgépben futó szoftver verziója</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DPS" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>A pénztárgépben futó szoftver SHA256 algoritmussal kiszámított ellenőrző kódja</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VCC">
        <xsd:annotation>
          <xsd:documentation>Üzembe helyezési kód</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:length value="16"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="MIS">
        <xsd:annotation>
          <xsd:documentation>A legutóbbi beavatkozást végző műszerész igazolvány száma</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="12"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="APN">
        <xsd:annotation>
          <xsd:documentation>Pénztárgép AP száma</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="APszam_tip"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HSH" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>Ellenőrző kód</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="STA_tip">
    <xsd:annotation>
      <xsd:documentation>pénztárgép rendszerállapot statisztika</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RSR" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>Naplón belüli folyamatos rekordsorszám 1-től</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DTS" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>Időbélyeg</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SST">
        <xsd:annotation>
          <xsd:documentation>Pénztárgép állapot</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="0|1|2"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SFL" type="numNNI10">
        <xsd:annotation>
          <xsd:documentation>Az AEE egységben működő SD kártyán lévő szabad tároló kapacitás (Kbyte)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SSG">
        <xsd:annotation>
          <xsd:documentation>Az AEE-ben működő modem által mért hálózati térerőt (-1)dBm - (-199)dBm-ig terjedő skálán negatív egész számmal ábrázolva.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="numNI3"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SAC">
        <xsd:annotation>
          <xsd:documentation>Akkumulátor töltöttség egész számmal, vezető nullák nélkül (%)</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="numNNI3"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SMT" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>GSM hálózatból kiolvasott pontos idő</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SCC" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Pénztárgépben az utolsó STA rekord küldése óta jelentkezett hibák belső kódjainak listája.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="(R[0-9]{2}.+)(~R[0-9]{2}.+)*"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HSH" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>Ellenőrző kód</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OPR_tip">
    <xsd:annotation>
      <xsd:documentation>Apénztárgép mûszaki mérõszámai</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RSR" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>Naplón belüli folyamatos rekordsorszám 1-től</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DTS" type="Datum_tip">
        <xsd:annotation>
          <xsd:documentation>Időbélyeg</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ODS" type="numNNI12">
        <xsd:annotation>
          <xsd:documentation>A legutóbbi bekapcsolás óta a modem által küldött bájtok száma, beleértve a megszakadt kapcsolatok adatait is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ODR" type="numNNI12">
        <xsd:annotation>
          <xsd:documentation>A legutóbbi bekapcsolás óta a modem által fogadott bájtok számát, beleértve a megszakadt kapcsolatok adatait is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ODF" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>A legutóbbi bekapcsolás óta a mobil adatkapcsolat kiépítésére tett sikertelen kísérletek száma.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OSF" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>A legutóbbi bekapcsolás óta az SSL adatcsatorna felépítésére tett sikertelen kísérletek száma.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OCC">
        <xsd:annotation>
          <xsd:documentation>AEE egység belső mérőszámai,ahol ON a belső mérőszám azonosítóját, OE a mért paraméter értékét jelenti.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="99">
            <xsd:element name="NCC" type="szov20_tip"/>
            <xsd:element name="ECC" type="szov50_tip"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="HSH" type="HSH_tip">
        <xsd:annotation>
          <xsd:documentation>Ellenőrző kód</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TUD_tip">
    <xsd:annotation>
      <xsd:documentation>Idő állításkor</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RSR" type="num5e_tip">
        <xsd:annotation>
          <xsd:documentation>Naplón belüli folyamatos rekordsorszám 1-től</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OLD" type="Datum_tip"/>
      <xsd:element name="NEW" type="Datum_tip"/>
      <xsd:element name="CHT" type="numNNI10"/>
      <xsd:element name="CAS">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="1|2|3|4|5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HSH" type="HSH_tip"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="P10E">
    <xsd:annotation>
      <xsd:documentation>Egyszerûsített számla</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="RekFej">
        <xsd:sequence>
          <xsd:element name="NSZ">
            <xsd:annotation>
              <xsd:documentation>a bizonylat száma 1234/00001 formátumban</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:pattern value="[0-9]{4}/[0-9]{5}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="IID" type="ESZEA_tip" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>az egyszerűsített számla egyedi azonosítója nnnnnnnnn/nnnn/nnnnn formátumban</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ITL" type="Tetel" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A bizonylaton szereplő összes tételre vonatkozóan, tételenként</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DTL" type="Tetel" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A bizonylaton szereplő összes törölt tételre vonatkozóan, tételenként</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="PMD">
            <xsd:annotation>
              <xsd:documentation>FM1: forint készpénzzel fizetett összeg, FMV: valutában fizetett összeg, FM2: bankkártyával fizetett összeg, FME: egészségpénztáron keresztül fizetett összeg, FM3: egyéb módon (utalvány, stb.) fizetett összeg</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="FM1" type="num20nt_tip" minOccurs="0"/>
                <xsd:element name="FMV" type="Valuta_adatok" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="FM2" type="num20t_tip" minOccurs="0"/>
                <xsd:element name="FME" type="num20t_tip" minOccurs="0"/>
                <xsd:element name="FM3" type="num20t_tip" minOccurs="0"/>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="GYJ" type="GyujtoF">
            <xsd:annotation>
              <xsd:documentation>A bizonylat tartalma forgalmi gyűjtőnként, kizárólag az adott bizonylaton ténylegesen használt gyűjtőkre vonatkozóan</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SUM" type="num20t_tip">
            <xsd:annotation>
              <xsd:documentation>A bizonylaton szereplő bruttó összeg, egész forintban.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="TAX" type="GyujtoA">
            <xsd:annotation>
              <xsd:documentation>Az adott bizonylat tartalma ÁFA-gyűjtőnként bontva</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DCN" type="num20nt_tip" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A bizonylaton szereplő összes engedmény egész forintban számolva (pozitív egész szám vagy nulla).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="RND" type="num20e_tip" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A bizonylaton szereplő kerekítés összege, egész forintban számolva, szükség esetén negatív előjellel, ha van ilyen adat.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="TRA" type="num20t_tip" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Kétvállalkozós pénztárgépeknél használt mező (átvitel)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="RCV" type="num20t_tip" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Kétvállalkozós pénztárgépeknél használt mező (áthozat)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
